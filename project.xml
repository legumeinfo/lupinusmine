<project type="bio">
  <property name="target.model" value="genomic"/>
  <property name="common.os.prefix" value="common"/>
  <property name="intermine.properties.file" value="lupinusmine.properties"/>
  <property name="default.intermine.properties.file" location="../default.intermine.integrate.properties"/>
  <sources>

    <!-- Lupinus/albus/about_this_collection -->
    <source name="Lupinus-albus-description" type="lis-description" dump="true" >
      <property name="dataSetName" value="Lupinus-albus-description" />
      <property name="dataSetUrl" value="https://data.legumeinfo.org/Lupinus/albus/about_this_collection/" />
      <property name="dataSetDescription" value="Lupinus albus organism and strain information." />
      <property name="src.data.dir" location="/home/shokin/data/v2/Lupinus/albus/about_this_collection" />
    </source>
    <!-- Lupinus/angustifolius/about_this_collection -->
    <source name="Lupinus-angustifolius-description" type="lis-description" dump="true" >
      <property name="dataSetName" value="Lupinus-angustifolius-description" />
      <property name="dataSetUrl" value="https://data.legumeinfo.org/Lupinus/angustifolius/about_this_collection/" />
      <property name="dataSetDescription" value="Lupinus angustifolius organism and strain information." />
      <property name="src.data.dir" location="/home/shokin/data/v2/Lupinus/angustifolius/about_this_collection" />
    </source>
    
    <!-- Lupinus/albus/genomes/Amiga.gnm1.F4NR -->
    <source name="Amiga.gnm1.F4NR" type="lis-genome" dump="true">
      <property name="dataSetUrl" value="https://data.legumeinfo.org/Lupinus/albus/genomes/Amiga.gnm1.F4NR/" />
      <property name="lis-genome.className" value="org.intermine.model.bio.Chromosome" />
      <property name="lis-genome.includes" value="README.Amiga.gnm1.F4NR.yml,lupal.Amiga.gnm1.F4NR.genome_main.fna.gz" />
      <property name="src.data.dir" location="/home/shokin/data/v2/Lupinus/albus/genomes/Amiga.gnm1.F4NR"/>
    </source>
    <!-- Lupinus/angustifolius/genomes/Tanjil.gnm1.Qq0N -->
    <source name="Tanjil.gnm1.Qq0N" type="lis-genome" dump="true">
      <property name="dataSetUrl" value="https://data.legumeinfo.org/Lupinus/angustifolius/genomes/Tanjil.gnm1.Qq0N/" />
      <property name="lis-genome.className" value="org.intermine.model.bio.Chromosome" />
      <property name="lis-genome.includes" value="README.Tanjil.gnm1.Qq0N.yml,lupan.Tanjil.gnm1.Qq0N.genome_main.fna.gz" />
      <property name="src.data.dir" location="/home/shokin/data/v2/Lupinus/angustifolius/genomes/Tanjil.gnm1.Qq0N"/>
    </source>

    <!-- Lupinus/albus/annotations/Amiga.gnm1.ann1.3GKS -->
    <source name="Amiga.gnm1.ann1.3GKS" type="lis-annotation" dump="true">
      <property name="dataSetUrl" value="https://data.legumeinfo.org/Lupinus/albus/annotations/Amiga.gnm1.ann1.3GKS/" />
      <property name="src.data.dir" location="/home/shokin/data/v2/Lupinus/albus/annotations/Amiga.gnm1.ann1.3GKS"/>
    </source>
    <!-- Lupinus/angustifolius/annotations/Tanjil.gnm1.ann1.nnV9 -->
    <source name="Tanjil.gnm1.ann1.nnV9" type="lis-annotation" dump="true">
      <property name="dataSetUrl" value="https://data.legumeinfo.org/Lupinus/angustifolius/annotations/Tanjil.gnm1.ann1.nnV9/" />
      <property name="src.data.dir" location="/home/shokin/data/v2/Lupinus/angustifolius/annotations/Tanjil.gnm1.ann1.nnV9"/>
    </source>

    <!-- Lupinus/angustifolius/synteny/Tanjil.gnm1.syn.TwnC -->
    <source name="Tanjil.gnm1.syn.TwnC" type="lis-synteny" dump="true">
      <property name="dataSetUrl" value="https://data.legumeinfo.org/Lupinus/angustifolius/synteny/Tanjil.gnm1.syn.TwnC/" />
      <property name="src.data.dir" location="/home/shokin/data/v2/Lupinus/angustifolius/synteny/Tanjil.gnm1.syn.TwnC" />
    </source>

    <!-- ############### ONTOLOGIES etc. ############### -->

    <!-- sequence ontology -->
    <source name="so" type="so">
      <property name="src.data.file" location="/home/shokin/data/SO-Ontologies/Ontology_Files/so-simple.obo"/>
    </source>
    
    <!-- gene ontology -->
    <source name="go" type="go">
      <property name="src.data.file" location="/home/shokin/data/gene-ontology/go-basic.obo"/>
    </source>

    <!-- INTERPRO -->
    <source name="interpro" type="interpro">
      <property name="src.data.dir" location="/home/shokin/data/InterPro/"/>
    </source>

    <!-- InterPro2GO -->
    <source name="interpro-go" type="interpro-go">
      <property name="src.data.dir" location="/home/shokin/data/InterPro/ontology/"/>
    </source>

    <!-- Pfam terms -->
    <source name="pfam-term" type="pfam-term">
      <property name="src.data.dir" location="/home/shokin/data/Pfam/"/>
    </source>

    <!-- PANTHER terms -->
    <source name="panther-term" type="panther-term">
      <property name="src.data.dir" location="/home/shokin/data/PANTHER/"/>
    </source>
    
    <!-- LEGUMES/Fabaceae/genefamilies/legume.genefam.fam1.M65K/ -->
    <source name="legume.genefam.fam1.M65K" type="lis-genefamily" dump="true">
      <property name="dataSetUrl" value="https://data.legumeinfo.org/LEGUMES/Fabaceae/genefamilies/legume.genefam.fam1.M65K/" />
      <property name="src.data.dir" location="/home/shokin/data/v2/LEGUMES/Fabaceae/genefamilies/legume.genefam.fam1.M65K"/>
    </source>

    <!-- LEGUMES/Fabaceae/genefamilies/legume.genefam.fam1.M65K/legume.genefam.fam1.M65K.trees_ML_rooted/ -->
    <source name="legume.genefam.fam1.M65K.trees_ML_rooted" type="lis-phylotree" dump="true">
      <property name="dataSetUrl" value="https://data.legumeinfo.org/LEGUMES/Fabaceae/genefamilies/legume.genefam.fam1.M65K/" />
      <property name="src.data.dir" location="/home/shokin/data/v2/LEGUMES/Fabaceae/genefamilies/legume.genefam.fam1.M65K"/>
    </source>

  </sources>


  <post-processing>

    <!-- IM CORE -->
    <post-process name="do-sources" />
    <post-process name="create-utr-references"/>
    <post-process name="create-references" />
    <post-process name="make-spanning-locations"/>
    <post-process name="create-chromosome-locations-and-lengths"/>
    <post-process name="create-intergenic-region-features"/>
    <post-process name="transfer-sequences"/>
    <post-process name="populate-child-features"/>
    <post-process name="create-gene-flanking-features"/>

    <!-- LIS -->
    <post-process name="lis-create-genefamilytally" />
    <post-process name="lis-populate-publications" />

    <!-- VIEWS AND INDEXES -->
    <post-process name="create-location-overlap-index"/>
    <post-process name="create-overlap-view" />
    <post-process name="create-attribute-indexes"/>
    <post-process name="summarise-objectstore"/>
    <post-process name="create-autocomplete-index"/>
    <post-process name="create-search-index"/>

  </post-processing>

</project>
